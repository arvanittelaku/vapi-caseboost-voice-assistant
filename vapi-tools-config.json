        "type": "object",
        "properties": {
          "notes": {
            "description": "Notes about the customer's response",
            "type": "string"
          },
          "status": {
            "description": "New appointment status: 'confirmed', 'cancelled', or 'needs_reschedule'",
            "type": "string",
            "enum": [
              "confirmed",
              "cancelled",
              "needs_reschedule"
            ]
          },
          "contactId": {
            "description": "The GHL contact ID",
            "type": "string"
          },
          "appointmentId": {
            "description": "The appointment ID to update",
            "type": "string"
